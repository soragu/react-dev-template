{
    "presets": [
        ["es2015", { "loose": true }],
        "stage-1",
        "react"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "babel-plugin-webpack-alias",
                    {
                        "config": "./webpack.dev.config.js"
                    }
                ]
            ]
        },
        "production": {
            "plugins": [
                [
                    "transform-react-remove-prop-types", 
                    {
                        "ignoreFilenames": ["node_modules"]
                    }
                ]
            ]
        }
    }
}