{
  "name": "react-dev-template",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/soragu/react-dev-template.svg?branch=master)](https://travis-ci.org/soragu/react-dev-template) [![Coverage Status](https://coveralls.io/repos/github/soragu/react-dev-template/badge.svg)](https://coveralls.io/github/soragu/react-dev-template)",
  "main": "src/main.js",
  "scripts": {
    "test": "NODE_ENV=test mocha test/.setup.js --recursive test --reporter mochawesome",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- test/.setup.js --recursive test",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "start": "http-server ./dist -p 8080 -o",
    "start:dev": "webpack-dev-server --progress --color --config webpack.dev.config.js",
    "build:js": "NODE_ENV=production webpack --color --progress",
    "build:js:dev": "NODE_ENV=dev webpack --color --progress --display-modules --config webpack.dev.config.js",
    "lint": "eslint --ext .js,.jsx src test"
  },
  "keywords": [
    "react"
  ],
  "author": "cgu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.2",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "mirror-creator": "^1.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soragu/react-dev-template.git"
  },
  "bugs": {
    "url": "https://github.com/soragu/react-dev-template/issues"
  },
  "homepage": "https://github.com/soragu/react-dev-template#readme"
}
